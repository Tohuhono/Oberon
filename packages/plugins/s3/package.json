{
  "name": "@oberoncms/plugin-s3",
  "version": "0.0.1",
  "author": "Ahmed Rowaihi @sudorw",
  "license": "MIT",
  "description": "An puck component for embeding s3 assets made for OberonCMS",
  "keywords": [
    "oberon",
    "oberoncms",
    "cms",
    "puck",
    "block",
    "component",
    "nextjs",
    "react",
    "s3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tohuhono/Oberon.git",
    "directory": "oberoncms/s3"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "node": "./dist/server.js"
    },
    "./plugin": "./dist/plugin.js",
    "./helper": "./dist/s3/helper.js"
  },
  "bin": {
    "odt-init": "./scripts/initialise.mjs",
    "ost-prepare": "./scripts/prepare.mjs"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "node ../../scripts/clean-package.mjs",
    "dev": "vite build --watch",
    "wait": "wait-on ./dist/version && echo done",
    "wait:clean": "rimraf ./dist/version",
    "lint": "eslint .",
    "tsc": "tsc --pretty"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/lib-storage": "^3.578.0",
    "@aws-sdk/s3-request-presigner": "^3.577.0",
    "@oberoncms/core": "workspace:*",
    "@radix-ui/react-icons": "^1.3.0",
    "image-size": "1.1.1",
    "next": "^14.2.3",
    "react-dropzone": "^14.2.3",
    "server-only": "^0.0.1"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@measured/puck": "^0.14.2",
    "@tohuhono/dev": "workspace:*",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "eslint": "^8.57.0",
    "typescript": "5.4.2",
    "vite": "^5.1.6"
  }
}
