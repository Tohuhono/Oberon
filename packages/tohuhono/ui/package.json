{
  "name": "@tohuhono/ui",
  "version": "0.14.2",
  "author": "Tohuhono ltd",
  "license": "MIT",
  "description": "A UI library used by @tohuhono and @oberoncms packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tohuhono/Oberon",
    "directory": "packages/tohuhono/ui"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "odt clean",
