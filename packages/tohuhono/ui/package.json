    "./select": "./dist/select.js",
    "./separator": "./dist/separator.js",
    "./spinner": "./dist/spinner.js",
    "./switch": "./dist/switch.js",
    "./table": "./dist/table.js",
    "./textarea": "./dist/textarea.js",
    "./theme": "./dist/theme.js",
    "./toast": "./dist/toast.js",
    "./tooltip": "./dist/tooltip.js",
    "./tailwind.css": "./tailwind.css"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@config/eslint": "workspace:*",
    "@config/scripts": "workspace:*",
    "@config/typescript": "workspace:*",
    "@config/vite": "workspace:*",
    "@radix-ui/react-icons": "^1.3.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tohuhono/utils": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "input-otp": "^1.4.2",
    "next": "^16.1.6",
    "react-day-picker": "^9.13.2",
    "react-hook-form": "^7.71.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "5.9.3",
    "vite": "^7.3.1"
  }
