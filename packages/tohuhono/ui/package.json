    "generate": "pnpx shadcn-ui@latest add --",
    "wait": "wait-on ./dist/version && echo done",
    "wait:clean": "rimraf ./dist/version",
    "lint": "eslint .",
    "tsc": "tsc --pretty"
  },
  "files": [
    "dist",
    "src",
    "tailwind.css",
    "CHANGELOG*",
    "README*",
    "LICENSE*"
  ],
  "exports": {
    "./avatar": "./dist/avatar.js",
    "./button": "./dist/button.js",
    "./calendar": "./dist/calendar.js",
    "./card": "./dist/card.js",
    "./checkbox": "./dist/checkbox.js",
    "./command": "./dist/command.js",
    "./date": "./dist/date.js",
    "./dropdown-menu": "./dist/dropdown-menu.js",
    "./dialog": "./dist/dialog.js",
    "./form": "./dist/form.js",
    "./grid": "./dist/grid.js",
    "./icons": "./dist/icons.js",
    "./input": "./dist/input.js",
    "./input-otp": "./dist/input-otp.js",
    "./label": "./dist/label.js",
    "./navigation-menu": "./dist/navigation-menu.js",
    "./popover": "./dist/popover.js",
    "./prose": "./dist/prose.js",
    "./radio-group": "./dist/radio-group.js",
